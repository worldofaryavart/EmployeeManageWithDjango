{
  "version": 2,
  "functions": {
    "api/employeemanagement/wsgi.py": {
      "maxLambdaSize": "15mb"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/employeemanagement/wsgi.py"
    }
  ]
}
